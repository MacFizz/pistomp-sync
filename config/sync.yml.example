version: 1

remote:
  name: nc
  base_path: pistomp

paths:
  pedalboards:
    enabled: true
    local: /home/pistomp/data/.pedalboards
    remote: pedalboards
    mode: bidirectional

  banks:
    enabled: true
    local: /home/pistomp/data/banks.json
    remote: banks.json
    mode: bidirectional

  favorites:
    enabled: true
    local: /home/pistomp/data/favorites.json
    remote: favorites.json
    mode: bidirectional

  user_files:
    enabled: true
    local: /home/pistomp/data/user-files
    remote: user-files
    mode: upload
